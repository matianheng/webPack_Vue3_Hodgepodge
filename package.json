  "main": "index.js",
  "comments": "--注释：命令 dev 执行根目录webpack.config.js\\ 命令 devqa执行目录config/webpack.dev.conf.js运行命令 根据不同环境进行选择执行\\命令 start 自定义执行命令",
  "comments2": "--注释：命令 build 执行根目录webpack.config.js\\ 命令 devqa执行目录config/webpack.dev.conf.js运行命令 根据不同环境进行选择执行",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack serve --mode development",
    "start": "npm run webpackDev",
    "devqa": "webpack serve --config config/webpack.dev.conf.js",
    "webpackDev": "export NODE_ENV=development && webpack serve --config build/webpack.dev.js --mode development",
    "build": "webpack",
    "build_prod": "export NODE_ENV=production && webpack --config ./build/webpack.prod.js --mode production",
    "build_qa": "webpack  --config config/webpack.dev.conf.js"
  },
